<div class="shift_new" style="background-image:url(<%= asset_path "note2.png" %>)">

<div class="contents row">
  <div class="container">
    <div id="list_input" class="content_line" style="background-image:url(<%= asset_path "head011_02.png" %>)">ログインする</div>
    <%= form_with model: @user, url: user_session_path, id: 'new_user', class: 'new_user', local: true do |f| %>
      <div class="field">
        <%= f.label :ニックネーム %><br />
        <%= f.text_field :nickname, autofocus: true %>
      </div>
      
      <div class="field">
        <%= f.label :email %><br />
        <%= f.email_field :email, autofocus: true %>
      </div>

      <div class="field">
        <%= f.label :パスワード %><em>(6文字以上)</em><br />
        <%= f.password_field :password, autocomplete: "off" %>
      </div>

      <div class="actions">
        <%= f.submit "ログイン" %>
      </div>
    <% end %>
  </div>
</div>
</div>